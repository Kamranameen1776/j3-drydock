{
  "name": "@j3-drydock/api",
  "version": "3.242.131",
  "description": "DryDock API",
  "main": "src/index.ts",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://jibe-erp@dev.azure.com/jibe-erp/JiBe/_git/j3-drydock"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.4",
    "@types/cors": "2.8.15",
    "@types/express": "4.17.20",
    "@types/jest": "29.5.7",
    "@types/lodash": "4.14.200",
    "@types/multer": "1.4.11",
    "@types/node": "14.18.63",
    "@types/supertest": "2.0.15",
    "@types/swagger-ui-express": "3.0.0",
    "@types/uuid": "9.0.6",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "concurrently": "4.1.2",
    "eslint": "8.52.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "jest": "29.7.0",
    "jest-cli": "29.7.0",
    "nodemon": "2.0.22",
    "prettier": "2.8.8",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "axios": "1.6.0",
    "body-parser": "1.20.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "cors": "2.8.5",
    "css-select": "5.1.0",
    "dd-trace": "4.26.0",
    "decimal.js": "10.4.3",
    "dom-serializer": "2.0.0",
    "domhandler": "5.0.3",
    "domutils": "3.1.0",
    "exceljs": "4.4.0",
    "express": "4.18.2",
    "htmlparser2": "9.1.0",
    "http-status-codes": "1.4.0",
    "j2utils": "3.2309.233",
    "j3-express-auto-routes": "22.12.1",
    "lodash": "4.17.21",
    "moment": "2.29.4",
    "mssql": "6.4.1",
    "multer": "1.4.5-lts.1",
    "pm2": "5.3.0",
    "swagger-ui-express": "5.0.0",
    "tsoa": "5.0.0",
    "typeorm": "0.2.25",
    "uuid": "3.4.0"
  },
  "scripts": {
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "migrations": "node -r dotenv/config build/cli/migration_run.js",
    "migration:create": "typeorm migration:create -n",
    "start": "ts-node src/index.ts",
    "start:build": "node -r dotenv/config build/index.js",
    "local": "nodemon -r dotenv/config src/index.ts",
    "localpm2": "pm2 start . -n %npm_package_name% --watch && concurrently --kill-others \"tsc -w\" \"pm2 logs\"",
    "test": "jest",
    "test:coverage": "npm run test -- --coverage",
    "test:integration": "npm run test -- --c jest.integration.config.js",
    "test:watch": "jest --watch",
    "build": "tsc -p ./tsconfig.build.json",
    "swagger": "tsoa spec"
  },
  "nodemonConfig": {
    "watch": [
      "src/**/*.ts"
    ],
    "ignore": [
      "src/**/*.spec.ts"
    ],
    "execMap": {
      "ts": "ts-node"
    }
  }
}
